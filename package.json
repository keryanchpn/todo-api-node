  },
  "devDependencies": {
    "@stoplight/spectral-cli": "^6.15.0",
    "@stoplight/spectral-rulesets": "^1.21.0",
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.1",
    "jest": "^30.2.0",
